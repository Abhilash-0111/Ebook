<vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="40dp"
    android:height="40dp"
    android:viewportHeight="12"
    android:viewportWidth="12">


  <!-- Rotate the canvas, then translate, then scale, then draw the pause icon. -->
  <group android:scaleX="1" android:pivotX="6" android:pivotY="6">
    <group android:rotation="90" android:pivotX="6" android:pivotY="6">
      <group android:translateX="2">
        <!-- This path draws an orange triangular play icon. -->
        <path
            android:name="play_path"
            android:strokeWidth="0.1"
            android:fillColor="#00796B"
            android:strokeColor="#000"
            android:pathData="M 4,2.5 L 4,9.5 L 9.9,6 Z"/>
      </group>
    </group>
  </group>

  <group android:scaleX="0.5" android:pivotX="-4" android:pivotY="6">
    <group android:rotation="0" android:pivotX="6" android:pivotY="6">
      <group android:translateX="2">
        <!-- This path draws an orange triangular play icon. -->
        <path
            android:strokeColor="#00796B"
            android:strokeWidth="0.5"
            android:pathData="M14,0 l0,28 M0,14 l28,0"/>
      </group>
    </group>
  </group>





</vector>

